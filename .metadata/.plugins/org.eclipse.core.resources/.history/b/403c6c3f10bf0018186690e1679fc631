package LinkedList;

public class newlist {
	
	Node head;
	static int index = 1 ;
	
	public class Node {
		
		int data;
		Node next;
		
		public Node(int d) {
			// TODO Auto-generated constructor stub
			data = d;
			next = null;
			System.out.println("\nNode "+ index + " created");
			index++;
		}
	
	}
	
	public void push(int a) {
		
		Node nodeN = new Node(a);
		
		nodeN.next= head;
		head = nodeN;
		
	}
	
	public void disp_list() {
		
		Node tnode = head; 
	    while (tnode != null) 
	    { 
	        System.out.print(tnode.data+" "); 
	        tnode = tnode.next; 
	    }
	}
	
	public void append(int a) {
		
		Node nodeN = new Node(a);
		
		Node tnode = head; 
		
		nodeN.next=null;
		
	    while (tnode.next != null) 
	    { 
	      
	       tnode = tnode.next; 
	    }
	    	
	    tnode.next=nodeN;
	}
	
}






