package test;

class Geeks
{
	final int a ;
	Geeks()
	{
		this.a=0;
	}
	
	Geeks(int i)
	{
		this.a=i;
	}
	
    void display()
       {
           System.out.println("Hello World!");
       }
}

public class newtest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		Geeks obj = new Geeks(2);
		 
        obj.display();
        System.out.println("Value of a =" + obj.a);
	}

}
