package LinkedList;

public class newlist {
	
	Node head;
	static int index;
	
	public class Node {
		
		int data;
		Node next;
		
		public Node(int d) {
			// TODO Auto-generated constructor stub
			data = d;
			next = null;
			System.out.println("Node "+ index + "crated");
			index++;
		}
	}
	
	
}
